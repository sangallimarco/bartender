{
    "extends": ["tslint:latest", "tslint-config-prettier"],
    "linterOptions": {
        "exclude": [
            "node_modules/**/*.ts"
        ]
    },
    "rules": {
        "interface-name": [true, "never-prefix"],
        "ordered-imports": false,
        "no-namespace": false,
        "no-require-imports": false,
        "object-literal-sort-keys": false,
        "no-var-requires": false,
        "no-console": false
    }
}